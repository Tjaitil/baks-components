{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "lib/**/*", "lib/**/*.vue", "lib/**/*.ce.vue", "src/**/*"],
  "exclude": ["lib/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
