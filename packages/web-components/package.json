{
  "name": "baks-components-web",
  "version": "0.3.3",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Kjetil Baksaas",
  "license": "MIT",
  "description": "A collection of web components for baks components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tjaitil/baks-components.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "types": "vue-tsc -p tsconfig.build.json",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:css": "postcss -r bundle.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/vite": "^4.0.8",
    "baks-components-styles": "^0.5.0",
    "glob": "^10.3.12",
    "lit": "^3.1.4",
    "tailwindcss": "^4.0.8",
    "uuid": "^9.0.1",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.26"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/blocks": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@storybook/web-components": "^8.2.1",
    "@storybook/web-components-vite": "^8.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "storybook": "^8.2.1",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
